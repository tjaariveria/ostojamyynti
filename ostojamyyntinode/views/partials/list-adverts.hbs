{{#if list}}
{{#each (reverseList list)}}
<div class="ilmoitukset">
    <div class="ilmoitus" key={{ilmoitus_id}}>
        <div>
            <h1>{{ilmoitusLaji ilmoitus_laji}}: {{ilmoitus_nimi}}</h1>
            <h3>{{ilmoitus_kuvaus}}</h3>
        </div>
        <div>
            <h5>Ilmoittaja: {{findUser ../users ilmoittaja_id}} Ilmoitus jätetty: {{formatDate ilmoitus_paivays}}</h5>

            
            {{#if (isAdmin ../user.kayttaja_taso)}}
            <p>jee</p>
            {{/if}}
            
        </div>
    </div>
</div>
{{/each}}
{{else}}
<div>
    <p>
    <h2 style="text-align: center;">Haku ei tuottanut yhtään tulosta :/</h2>
    </p>
</div>
{{/if}}